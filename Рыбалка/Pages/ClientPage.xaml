<Page x:Class="Рыбалка.Pages.ClientPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Рыбалка.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ClientPage">

    <Grid Background="White">
        <Label FontSize="17" Margin="10 5 0 0" Name="countInList" VerticalAlignment="Top" HorizontalAlignment="Left" Content="1/2" FontFamily="Comic Sans MS"></Label>
        <TextBox FontFamily="Comic Sans MS" Name="search" Height="25" VerticalAlignment="Top" Margin="70 10 20 0 " TextChanged="sorting"></TextBox>
        <ScrollViewer Margin="0 50 0 10">
            <StackPanel Orientation="Vertical" Name="parrent"></StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
